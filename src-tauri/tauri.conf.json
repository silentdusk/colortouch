{
  "productName": "colortouch",
  "version": "1.0.0",
  "identifier": "dev.silentdusk.colortouch",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1520",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "Color Touch",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
